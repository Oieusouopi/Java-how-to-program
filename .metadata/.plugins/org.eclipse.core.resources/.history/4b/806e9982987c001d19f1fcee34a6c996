public class Exec2_20 {
	
	public static void main( String args[] ) {
		String caixa, oval;
		// 4 figuras caixa - oval - seta - losango
		// caixa
		caixa = "*********" + "\n" + "*       *" + "\n" + "*       *" + "\n" + "*       *" + "\n"
		  + "*       *" + "\n" + "*       *" + "\n" + "*       *" + "\n" + "*       *" + "\n"
		  + "*********";
		
		oval = "   ***   " + "\n" + " *     * " + "\n" + "*      *" + "\n" + "*      *"
			+ "\n" + "*      *" + "\n" + "*      *" + "\n" + "*      *" + "\n" + "*      *"
			+ "\n" + " *     * " + "\n" + "   ***   ";
		
		System.out.print(caixa + " " + oval);
	} // fim m√©todo main

} // fim da classe Exec2_19