public class DateTest {
	
	public static void main( String args[] ) {
		// instanciando a classe
		Date date = new Date(1,1,40);
		
		// mostrando os atributos date
		System.out.printf("o dia do date é: %d\no mês do date é: %d\no ano é: %d",
				date.getDia(), date.getMes(), date.getAno());
	}
	
} // fim da classe DateTest